  public T get() {
    try {
      return container.callInContext(new ContextualCallable<T>() {
        public T call(InternalContext context) {
          return construct(context, implementation);
        }
      });
    }
    catch (RuntimeException e) {
      throw e;
    }
    catch (Exception e) {
      throw new RuntimeException(e);
    }
  }

