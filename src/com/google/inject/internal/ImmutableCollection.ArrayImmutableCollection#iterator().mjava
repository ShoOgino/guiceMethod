    @Override public UnmodifiableIterator<E> iterator() {
      return new UnmodifiableIterator<E>() {
        int i = 0;
        public boolean hasNext() {
          return i < elements.length;
        }
        public E next() {
          if (!hasNext()) {
            throw new NoSuchElementException();
          }
          return elements[i++];
        }
      };
    }

