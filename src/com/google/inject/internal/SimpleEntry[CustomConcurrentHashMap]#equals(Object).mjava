  @Override
  public boolean equals(Object o) {
    if (!(o instanceof Map.Entry)) {
      return false;
    }

    Map.Entry e = (Map.Entry) o;
    return (getKey()==null ?
          e.getKey()==null : getKey().equals(e.getKey())) &&
         (getValue()==null ? e.getValue()==null : getValue().equals(e.getValue()));
  }

