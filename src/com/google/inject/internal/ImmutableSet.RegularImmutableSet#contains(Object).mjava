    @Override public boolean contains(Object target) {
      if (target == null) {
        return false;
      }
      for (int i = Hashing.smear(target.hashCode()); true; i++) {
        Object candidate = table[i & mask];
        if (candidate == null) {
          return false;
        }
        if (candidate.equals(target)) {
          return true;
        }
      }
    }

