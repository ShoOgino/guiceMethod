  public <T> Provider<T> scope(
      final Key<T> key, final Provider<T> provider) {
    return new Provider<T>() {
      @SuppressWarnings({"unchecked"})
      public T get() {
        T t = (T) inScopeObjectsMap.get(key);
        if (t == null) {
          t = provider.get();
          inScopeObjectsMap.put(key, t);
        }
        return t;
      }
    };
  }

