  public void testPrivateModuleInheritScanner_usingPrivateModule() {
    Injector injector =
        Guice.createInjector(
            NamedMunger.module(),
            new PrivateModule() {
              @Override
              protected void configure() {}

              @Exposed
              @TestProvides
              @Named("foo")
              String foo() {
                return "foo";
              }
            });
    assertMungedBinding(injector, String.class, "foo", "foo");
  }

