  public void testVoidProviderMethods() {
    try {
      Guice.createInjector(
          new AbstractModule() {

            @Provides
            void provideFoo() {}
          });
      fail();
    } catch (CreationException expected) {
      assertContains(
          expected.getMessage(),
          "1) Provider methods must return a value. Do not return void.",
          getClass().getName(),
          ".provideFoo(ProviderMethodsTest.java:");
    }
  }

